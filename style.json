{
	"QMainWindow":[
		{
			"tittle":"OOPP-2",
			"icon":":/images/logo.png",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "sizeGrip",
			"navigation":[
				{
					"minimize":"minimizeBtn",
					"close": "closeBtn",
					"restore":[
						{	
							"buttonName": "restoreBtn",
							"normalIcon": ":/icons/icon/square.svg",
							"maximizedIcon": ":/icons/icon/copy.svg"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
	"QCustomSlideMenu": [
		{
			"name": "leftMenuContainer",
			"defaultSize": [{
				"width": 45,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 45,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 150,
				"height": "parent"
			}],
			"toggleButton": [{
				"buttonName": "menuBtn",
				"icons": [{
					"whenMenuIsCollapsed": ":/icons/icon/align-justify.svg",
					"whenMenuIsExpanded": "::/icons/icon/chevron-left.svg"
				}]
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "centerMenuContainer",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 200,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "rightMenuContainer",
			"defaultSize": [{
				"width": 0,
				"height": "auto"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "auto"
			}],
			"expandedSize": [{
				"width": 200,
				"height": "auto"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "popupNotificationContainer",
			"defaultSize": [{
				"width": 0,
				"height": "auto"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "auto"
			}],
			"expandedSize": [{
				"width": 500,
				"height": 100
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}],
			"floatPosition": [
				{
					"relativeTo": "centralwidget",
					"position": "top-center",
					"shadow":[
						{
							"color": "#000",
							"blurRadius": 20,
							"xOffset": 0,
							"yOffset": 0
						}
					]
				}
			],
			"toggleButton": [{
				"buttonName": "notificationBtn"
			}]
		}
	],
	"QStackedWidget":[
		{
			"name":"mainPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"homeBtn": "page_8",
							"dataBtn": "page_9",
							"reportBtn": "page_10"
						}
					]
				}
			]
	
		},
		{
			"name":"centerMenuPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"settingsBtn": "page",
							"infoBtn": "page_2",
							"helpBtn": "page_3"
						}
					]
				}
			]
	
		},
		{
			"name":"rightMenuPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"profileBtn": "page_6",
							"moreMenuBtn": "page_2"
						}
					]
				}
			]
	
		}
	],
	"QPushButtonGroup": [
		{
			"Buttons": [
				"homeBtn",
				"dataBtn",
				"reportBtn"
			],
			"Style":[
				{
					"Active": "background-color: #1f232a",
					"NotActive": ""
				}
			]
		},
		{
			"Buttons": [
				"settingsBtn",
				"infoBtn",
				"helpBtn"
			],
			"Style":[
				{
					"Active": "background-color: #2c313c",
					"NotActive": ""
				}
			]
		}
	]
}